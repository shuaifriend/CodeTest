
.todo-header{
  position: fixed;
  left: 0;
  right: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align:center;
  -webkit-align-items:center;
          align-items:center;
  padding: 0 15px;
  font-size: 12px;
  color: #333333;
  height: 45px;
  box-shadow: -1px 1px 5px 0px rgba(0,0,0,.1);
  background-color: #FFFFFF;
  z-index: 10;
}

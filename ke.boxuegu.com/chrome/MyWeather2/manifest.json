{
  "manifest_version": 2,
  "name": "我的天气2",
  "version": "1.0.0",
  "description": "我的第一个Chrome扩展",
  "icons": {
    "16": "images/weather.png",
    "48": "images/weather.png",
    "128": "images/weather.png"
  },
  "browser_action": {
    "default_icon": "images/weather.png",
    "default_title": "我的天气2",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": ["*://tq.daodaoim.com/*", "notifications"]
}
